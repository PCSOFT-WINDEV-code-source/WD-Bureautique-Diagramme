   -
     name : MenuListeDossiersRécents
     procedure_id : 1880244499313580258
     type_code : 14
     code : |1-
      // Résumé : Liste les dossiers récemment utilisés
      // Syntaxe :
      //MenuListeDossiersRécents ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MenuListeDossiersRécents()
      	
      	tabDossiers est un tableau de chaines
      	
      	Sablier( Vrai )
      	
      	// Désactive l'affichage
      	ZR_EMPLACEMENTS_RECENTS..AffichageActif = Faux
      	BTN_ENREGISTRERSOUS_PARCOURIR..Visible = faux
      	
      	// Signale le début du remplissage
      	ZR_EMPLACEMENTS_RECENTS.DébutRemplissage()
      	
      	// Vide la liste
      	ZR_EMPLACEMENTS_RECENTS.SupprimeTout()
      	
      	// La MRU est déjà triée
      	POUR TOUT element stElement DE COL_PARAMETRAGE.MRU
      		
      		// Si l'extension du fichier ne correspond pas à celle paramétrée, on l'ignore
      		SI Position(COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR, "*." + fExtraitChemin(stElement.CheminComplet, fExtension)[[2 À]], 1, SansCasse + MotComplet) < 1 ALORS CONTINUER
      		
      		// Si le document n'existe plus, on l'ignore
      		si ffichierexiste(stElement.CheminComplet) = faux alors continuer
      		
      		dhDate est une dateheure = DateHeureUTCVersLocale( stElement.DateHeureAccès )
      		
      		// Vérifie si on a déjà rencontré ce dossier
      		si tabDossiers.Cherche(tccSansCasse, stElement.CheminComplet) > 0 ALORS
      			continuer
      		FIN
      		
      		// Mémorise le chemin
      		tabDossiers.Ajoute( stElement.CheminComplet )
      		
      		// Affiche le dossier
      		nLigne est un entier = ZR_EMPLACEMENTS_RECENTS.AjouteLigne( stElement.CheminComplet, DateVersChaîne(dhDate..PartieDate, maskDateDuréeRelative) )		
      		ZR_EMPLACEMENTS_RECENTS[nLigne].LIB_NOM_DOSSIER = DécomplèteRep(fExtraitChemin(stElement.CheminComplet, fDisque + fRépertoire))
      	FIN	
      	
      	// Signale la fin du remplissage
      	ZR_EMPLACEMENTS_RECENTS.FinRemplissage()
      	ZR_EMPLACEMENTS_RECENTS.SelectMoins()
      	
      	// Réactive l'affichage
      	BTN_ENREGISTRERSOUS_PARCOURIR..Visible = vrai
      	ZR_EMPLACEMENTS_RECENTS..AffichageActif = Vrai
      
      	Sablier( faux )
     type : 458752
   -
     name : OuvreSélecteurFichier
     procedure_id : 1880259175267826451
     type_code : 14
     code : |1-
      // Résumé : Ouvre le sélecteur de fichiers
      // Syntaxe :
      //[ <Résultat> = ] OuvreSélecteurFichier ( [<nMode> est entier [, <sRépertoireInitial> [, <sFiltre> est chaîne [, <sExtensionDéfaut> est chaîne]]]])
      //
      // Paramètres :
      //	nMode (entier - valeur par défaut=2) : Mode du sélecteur (constantes fSelXXX)
      //	sRépertoireInitial (valeur par défaut="") : Répertoire d'ouverture (par défaut dernier répertoire ouvert ou répertoire du document courant)
      //	sFiltre (chaîne ANSI - valeur par défaut="") : Filtre du sélecteur de fichiers
      // 	sExtensionDéfaut (chaîne ANSI - valeur par défaut="") : Extension par défaut (sans le *.)
      // Valeur de retour :
      // 	chaîne ANSI : Chemin sélectionné
      //
      PROCEDURE OuvreSélecteurFichier(nMode est un entier = fselOuvre, local sRépertoireInitial = "", local sFiltre est une chaine = "", local sExtensionDéfaut est une chaine = "") : chaine
      	
      	sFichier est une chaine
      	sRépertoireInitial = (sRépertoireInitial <> "" ? sRépertoireInitial sinon ChargeParamètre("Recent\LastPath", ( gsCheminDocument = "" ? sysrep( srMesDocuments ) sinon fExtraitChemin(gsCheminDocument, fDisque + fRépertoire) )))
      	
      	// Si on n'a pas fourni de filtre par défaut
      	si sFiltre = "" ALORS
      		sFiltre = "[%COL_PARAMETRAGE.DESCRIPTION_DOCUMENT%] ([%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%])" + TAB + COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR
      	FIN
      	
      	// Si on n'a pas fourni d'extension par défaut
      	si sExtensionDéfaut = "" alors
      		sExtensionDéfaut = extraitchaine(remplace( COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR , "*.", "" ), 1, ";")
      	fin
      		
      	// Ouvre le sélecteur de fichiers
      	sFichier = fSélecteur(sRépertoireInitial, gsNomDocumentOuvert, (nMode & fselCrée = fselcree ? "Enregistrer sous" sinon "Ouvrir"), "Tous les fichiers (*.*)" + TAB + "*.*" + RC + ...
      											  sFiltre, sExtensionDéfaut, nMode )
      	
      	// Si aucun fichier n'a été sélectionné
      	SI sFichier = "" ALORS renvoyer ""
      	
      	// Mémorise le chemin
      	SauveParamètre("Recent\LastPath", fExtraitChemin(sFichier, fDisque + fRépertoire))
      	
      renvoyer sFichier
     type : 458752
   -
     name : MenuFermeDocument
     internal_properties : CAAAAAgAAACm6X7eoSZQVwBFQzagMlNGmsZl2a8or+Dd92bWvB7hvdljJRj6quaRWpGJhDwvBG9MYSkLEzXHX4vvcIXS0pAD3rZolOP8rZO42x00ts6lVk2e2tmcOiLvoCZYgTVfnV9zkpL6u2/loU3ZNO0eZP8uANFP9uFgSj8GIDwUAduHNbC2
     procedure_id : 1880289703849652169
     type_code : 14
     code : |1-
      // Résumé : Ferme le document en cours
      // Syntaxe :
      //[ <Résultat> = ] MenuFermeDocument ( [<bFermeFenêtre> est booléen])
      //
      // Paramètres :
      //	bFermeFenêtre (booléen - valeur par défaut=1) : Vrai pour forcer la fermeture de la fenêtre, Faux sinon
      // Valeur de retour :
      // 	booléen : Vrai si la fermeture a été effectuée avec succès, Faux dans le cas contraire
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE MenuFermeDocument(local bFermeFenêtre est un booléen = vrai) : booléen
      	
      	// Si le document a été modifié
      	SI gbDocumentModifié ALORS
      		
      		//1 : Enregistrer
      		//2 : Ne pas enregistrer
      		//3 : Annuler
      		selon Dialogue(<§?128C491E00466BE7§>) 
      			
      			// Enregistrer
      			cas 1 
      			
      				// Sauvegarde le document
      				si MenuSauveDocument() = -1 ALORS
      					RENVOYER Faux
      				FIN
      		
      			// Ne pas enregistrer
      			cas 2
      				
      				// On ne fait rien
      			
      			// Annuler
      			cas 3
      				// On arrête tout et on reprend
      				renvoyer faux
      		FIN
      	FIN
      
      	// Déclenche la fermeture
      	SI DocumentAction.FermeDocument = Null _OU_ DocumentAction.FermeDocument( gsCheminDocument ) = Vrai ALORS
      			
      		// Mémorise le document dans la MRU
      		MRUAjoute( gsCheminDocument, gxDocPosition )
      	
      		// Ferme
      		si bFermeFenêtre alors ferme()	
      		RENVOYER vrai
      	fin
      	
      RENVOYER Faux
      	
      CAS EXCEPTION:
      	dbgAssertion( Faux, ExceptionInfo )
      	SI bFermeFenêtre ALORS ferme()
      	RENVOYER Faux
     type : 458752
   -
