              code : |1-
               
               bAssociation est un booléen = Val(MoiMême..Note)
               
               // Associe la ou les extensions déclarées au programme
               si AssocieExtensions( bAssociation ) = faux ALORS
               	// Le lancement a été annulé
               	retour
               FIN
               
               si bAssociation alors
               	
               	// Marque que l'association a été effectuée
               	sauveParamètre("FileExtAssociation", "Done")
               	
               	// Affiche un toast
               	ToastAffiche("Les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] s'ouvriront désormais avec [%ProjetInfo(piNomProjet)%]", toastCourt, cvMilieu)
               		
               sinon
               	
               	// Annule la marque que l'association a été effectuée
               	SauveParamètre("FileExtAssociation", "")
               	
               	// Affiche un toast
               	ToastAffiche("Les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] ne s'ouvriront plus avec [%ProjetInfo(piNomProjet)%]", toastCourt, cvMilieu)
               fin
               
               // Mise à jour du bouton
               MoiMême.ExécuteTraitement( trtinit )
               
               // Ferme le menu
               MenuFichierFerme()
