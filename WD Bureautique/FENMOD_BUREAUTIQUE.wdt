  -
    name : BTN_MENU_FICHIER
    identifier : 0x2a15aadd2fa4f0a2
    internal_properties : 
    properties :
     x : 1
     y : 36
     width : 80
     height : 31
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // Le bouton [Fichier] doit être au dessus de tout le reste
         BTN_MENU_FICHIER..Altitude = 1000
        type : 14
      -
        code : |1-
         
         ///////////////////
         // Affiche le menu
         ///////////////////
         
         // Mémorise le plan courant et les propriétés qui vont être modifiées
         nPlan est un entier 			 = mafenetre..plan	
         sImgSysClose est une chaine 	 = BTN_SYST_CLOSE..Image
         sImgSysRestore est une chaine 	 = BTN_SYST_RESTORE..Image
         sImgSysMaximize est une chaine 	 = BTN_SYST_MAXIMIZE..Image
         sImgSysIconize est une chaine 	 = BTN_SYST_ICONIZE..Image		
         sBkImgSysClose est une chaine 	 = BTN_SYST_CLOSE..Imagefond
         sBkImgSysRestore est une chaine  = BTN_SYST_RESTORE..Imagefond
         sBkImgSysMaximize est une chaine = BTN_SYST_MAXIMIZE..Imagefond
         sBkImgSysIconize est une chaine  = BTN_SYST_ICONIZE..Imagefond		
         	
         // Prépare l'affichage
         AnimationPrépare(MaFenêtre)
         			
         // Modifie les propriétés
         BTN_SYST_CLOSE..Image = Remplace((BTN_SYST_CLOSE..Image), "_light", "")
         BTN_SYST_RESTORE..Image = Remplace((BTN_SYST_RESTORE..Image), "_light", "")
         BTN_SYST_MAXIMIZE..Image = Remplace((BTN_SYST_MAXIMIZE..Image), "_light", "")
         BTN_SYST_ICONIZE..Image = Remplace((BTN_SYST_ICONIZE..Image), "_light", "")
         BTN_SYST_CLOSE..ImageFond = Remplace((BTN_SYST_CLOSE..ImageFond), "_light", "")
         BTN_SYST_RESTORE..ImageFond = Remplace((BTN_SYST_RESTORE..ImageFond), "_light", "")
         BTN_SYST_MAXIMIZE..ImageFond = Remplace((BTN_SYST_MAXIMIZE..ImageFond), "_light", "")
         BTN_SYST_ICONIZE..ImageFond = Remplace((BTN_SYST_ICONIZE..ImageFond), "_light", "")
         IMG_LOGO_WINDEV..visible = faux
         DISP_RACCOURCIS..visible = faux
         BTN_MENU_FICHIER..visible = faux
         RADTITRE..Couleur = GrisFoncé
         			
         // Affiche le plan du menu, sur [Informations]
         MaFenêtre..Plan = PLAN_INFORMATIONS
         MenuMarqueActif( BTN_MENU_INFOS )
         		
         // Le clic du bouton [RETOUR] devra restaurer les propriétés modifiées
         BTN_MENU_RETOUR..traitement[ trtclic ] = ___CacheMenu
         
         // Déclenche l'animation
         animationjoue(animRecouvrementDroite, 100ms)
         
         	procedure interne ___CacheMenu( bAnimation est un booléen = vrai)
         		
         		// Prépare l'affichage
         		si bAnimation ALORS
         			AnimationPrépare(MaFenêtre)
         		sinon
         			mafenetre..AffichageActif = Faux
         		FIN
         		
         		// Restaure les propriétés modifiées
         		BTN_SYST_CLOSE..Image = sImgSysClose
         		BTN_SYST_RESTORE..Image = sImgSysRestore
         		BTN_SYST_MAXIMIZE..Image = sImgSysMaximize
         		BTN_SYST_ICONIZE..Image = sImgSysIconize
         		BTN_SYST_CLOSE..ImageFond = sBkImgSysClose
         		BTN_SYST_RESTORE..ImageFond = sBkImgSysRestore
         		BTN_SYST_MAXIMIZE..ImageFond = sBkImgSysMaximize
         		BTN_SYST_ICONIZE..ImageFond = sBkImgSysIconize
         		RADTITRE..Couleur = blanc
         		IMG_LOGO_WINDEV..Visible = vrai
         		DISP_RACCOURCIS..Visible = vrai
         		BTN_MENU_FICHIER..Visible = vrai
         		
         		BTN_MENU_OPTIONS..enfonce = Faux
         		BTN_MENU_EXPORTER..Enfoncé = Faux
         		BTN_MENU_IMPRIMER..Enfoncé = Faux
         		BTN_MENU_ENREGISTRER_SOUS..Enfoncé = faux
         		BTN_MENU_OUVRIR..Enfoncé = faux
         		BTN_MENU_NOUVEAU..Enfoncé = faux
         		BTN_MENU_INFOS..Enfoncé = faux
         		
         		// Restaure le plan original
         		MaFenêtre..Plan = nPlan
         		
         		// Déclenche l'animation
         		si bAnimation ALORS
         			AnimationJoue(animRecouvrementGauche, 100ms )
         		sinon
         			mafenetre..AffichageActif = vrai		
         		FIN
         		
         		// Annule le traitement de clic
         		BTN_MENU_RETOUR..Traitement[ trtClic ] = null
         	FIN
        type : 18
      -
        type : 161
        enabled : false
