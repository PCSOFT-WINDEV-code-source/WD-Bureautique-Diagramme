     name : ConfigBiblioAjoute
     internal_properties : CAAAAAgAAAATkpLNX73nXq4eh/klxhsTqmMvoNbu3rFUOztv6Ujf0gj/t1KonjFr2ABm0/gIgG3iH9Sjd9qmrzXg7tiqvutZTKxeucLI9Mb8BN+hurhsS7iCrJQSSn99a9YWmd1ok7t0cBNDprXSanBDVVm+U8YnUgRO1CkSzULalJevkE0rUsBo2h4ZG5wMia65la2hQsFoApXCMFnFiIRS1CY1KZmQuwG05EjSV5SBWdJuXa3Io4Nb+8roOv/AtW0=
     procedure_id : 1318412874621289421
     type_code : 14
     code : |1-
      // Résumé : Mémorise l'ajout d'une bibliothèque
      // Syntaxe :
      //ConfigBiblioAjoute (<sNom> est chaîne [, <nPos> est numérique [, <bPerso> est booléen [, <bCharge> est booléen [, <sChemin> est chaîne]]]])
      //
      // Paramètres :
      //	sNom (chaîne ANSI) : Nom de la bibliothèque
      //	nPos (numérique - valeur par défaut=-1) : Position dans la bibliothèque (dans le panneau)
      //	bPerso (booléen - valeur par défaut=0) : Bibliothèque standard ou perso ?
      //	bCharge (booléen - valeur par défaut=1) : Bibliothèque à afficher ou non ?
      //	sChemin (chaîne ANSI - valeur par défaut="") : Chemin du .wddiag si c'est une bibliothèque perso
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ConfigBiblioAjoute(local sNom est une chaine, local nPos est un numérique = -1, LOCAL bPerso est un booleen = faux, LOCAL bCharge est un booleen = vrai, local sChemin est une chaine = "")
      
      stElement	est un BiblioElement
      
      // Crée une entrée
      stElement.Libellé		= sNom
      stElement.Indice		= nPos
      stElement.Perso			= bPerso
      stElement.àCharger		= bCharge
      stElement.CheminComplet	= sChemin
      
      // Vérifie si cette bibliothèque est déjà dans la liste
      nIndice est un entier = Cherche(biblio, tcLinéaire + tccSansCasse, "Libellé", sNom)
      
      SI nIndice > 0 ALORS
      	// Mise à jour
      	Biblio[ nIndice ] = stElement
      sinon
      	// Ajout
      	biblio.Ajoute( stElement )		
      Fin
      
      // Sauve
      ConfigBiblioSauve()
     type : 458752
   -
     name : ConfigBiblioCharge
     internal_properties : CAAAAAgAAACzv9kmKuEmQ24QDY/AryMFrnX+t3TMW+oi2bQ9NqM+93hL0ExlC9fD7yDp51KmphMDFWUdtSJZR6tJjY2SPByP/yjHGZeS+2ZiLE9ydBSQ9bZBfJkr/v7BZoRb7DHyuH2i3Vgko/4TMmbebm3ILAu3rLdJ7Mj+9LsUmRSe1e/E6z+2PIEvh3bb2VxZ+P/s3+qinhV8qnrvVsjnnyS88sXSVAKrWIv/OTBmH0QLF0UNiIlUG11CFrm6
     procedure_id : 1318412874621355176
     type_code : 14
     code : |1-
      // Résumé : Charge la liste des bibliothèques à afficher
      // Syntaxe :
      //[ <Résultat> = ] ConfigBiblioCharge ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	tableau (BiblioElement) : Liste des bibliothèques
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE ConfigBiblioCharge() : tableau de BiblioElement
      
      tabBiblio		est un tableau de BiblioElement
      sJSON			est une chaine ansi
      sFichierBiblio	est une chaine	= fRepDonnéesUtilisateur() + [fSep] + "biblio.json"
      
      // Vérifie l'existence du fichier
      si fFichierExiste( sFichierBiblio ) = faux ALORS
      	RENVOYER tabBiblio
      FIN
      
      // Charge les données
      sJSON = fChargeTexte( sFichierBiblio )
      
      // Sauvegarde vide
      si sJSON = "" ALORS
      	RENVOYER tabBiblio
      FIN
      
      // Désérialise le contenu
      deserialise(tabBiblio, sJSON, psdJSON)
      
      // Recopie la liste
      renvoyer tabBiblio
      
      CAS EXCEPTION:
      RENVOYER tabBiblio
     type : 458752
