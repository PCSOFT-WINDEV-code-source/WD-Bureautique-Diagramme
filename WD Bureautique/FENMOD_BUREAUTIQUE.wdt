            -
              code : |1-
               
               // Si on n'a pas encore enregistré l'association
               si ChargeParamètre("FileExtAssociation", "") = "" alors
               
               	// On devra mettre en place l'association
               	moimeme..Libellé = "Associer les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] à [%ProjetInfo(piNomProjet)%]"
               	MoiMême..note    = "1"
               		
               sinon
               	
               	// On devra annuler l'association
               	MoiMême..Libellé = "Annuler l'association des fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] à [%ProjetInfo(piNomProjet)%]"
               	MoiMême..Note    = "0"
               fin
               	
               si enmodetest() = faux alors MoiMême..Bulle = MoiMême..Libellé
               moimeme..Largeur = textelargeur(moimeme, moimeme..libelle, tMonoligne) + CoordonnéeEditeurVersEcran(24 + 8)
               
               // Si l'UAC est actif et que l'utilisateur n'est pas administrateur
               si UACActif() _et_ UtilisateurEstAdministrateur() = faux ALORS
               	
               	MoiMême..Image = IMG_UAC
               FIN
              type : 14
