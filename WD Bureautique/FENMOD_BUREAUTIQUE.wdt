   -
     name : MenuListeBibliothèques
     procedure_id : 1318143730388325779
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // MenuListeBibliothèques ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      procédure MenuListeBibliothèques()
      
      
      Sablier( Vrai )
      
      // Désactive l'affichage
      ZR_BIBLIO..AffichageActif	= Faux
      
      // Signale le début du remplissage
      ZR_BIBLIO.DébutRemplissage()
      
      // Vide la liste
      ZR_BIBLIO.SupprimeTout()
      
      POUR TOUT stElement DE Biblio
      		
      	sType est une chaine
      	si stElement.Perso = faux alors
      		sType = "STANDARD"
      	sinon
      		sType = "PERSONNALISÉE"
      		// Si le document n'existe plus, on l'ignore
      		SI fFichierExiste(stElement.CheminComplet) = Faux ALORS CONTINUER
      	FIN
      	
      	// Charge les informations fichier du document				
      	nLigne	est un entier		= zr_biblio.AjouteLigne(stElement.Perso, sType, stElement.Libellé, stElement.àCharger,stElement.CheminComplet)
      	
      	
      FIN
      
      // Signale la fin du remplissage
      ZR_BIBLIO.FinRemplissage()
      ZR_BIBLIO.SelectMoins()
      
      ZR_BIBLIO..AffichageActif	= Vrai
      
      Sablier( Faux )
     type : 458752
   -
     name : AjoutNouvelleBibliothèque
     internal_properties : CAAAAAgAAAD/e1a1cmSSzZjvmjwhPipeo/FKP4WKv6mOu3oLgGGGMj3jgvO91BNF2PHYAWKyPCjU5SpU+wRlM8scXEFJ6lo/r8iTzoqv9NtDutKeJjL3APOk66mbFXVZ1LTCukIL77Vp+vMMu07ocjDX7/7RdGYOzdcLmQZaUCegqXWjfDm35gDItMFk2QbdKO88uKcwG56hkpek3OFkIAlthinNrzy3u/YcNYx0ZLG3bs9AkCcqcSopY4PHSvV2
     procedure_id : 1318144430486573752
     type_code : 14
     code : |1-
      // Résumé : Ajoute une nouvelle bibliothèque au champ Éditeur de diagrammes
      // Syntaxe :
      //[ <Résultat> = ] AjoutNouvelleBibliothèque (<sNom> est chaîne, <sRépertoire> est chaîne)
      //
      // Paramètres :
      //	sNom (chaîne ANSI) : Nom à donner à la bibliothèque
      //	sRépertoire (chaîne ANSI) : Répertoire contenant les fichiers svg
      // Valeur de retour :
      // 	chaîne ANSI : Fichier .wddiag sauvegardé 
      //
      PROCÉDURE AjoutNouvelleBibliothèque(sNom est une chaîne, sRépertoire est une chaîne )
      
      // Nouvelle bibliothèque à créer
      MaBibliothèque est un diagBibliothèque
      MaBibliothèque.Nom = sNom
      
      // Liste les fichiers .svg du répertoire "bibliothèque"
      sListeSVG est une chaîne
      sListeSVG = fListeFichier(sRépertoire + fSep() +"*.svg", frNonRécursif)
      
      // Pour chaque fichier
      POUR TOUTE CHAÎNE sUnChemin DE sListeSVG SÉPARÉE PAR RC
      	
      	// Crée un objet diagImage
      	MonImage est une diagImage
      	
      	// Mémorise le chemin complet du fichier SVG source
      	MonImage.Image		= sUnChemin
      	
      	// Pour le nom, il est possible d'utiliser :
      	// - une information contenue dans le fichier SVG (par exemple, le contenu de la balise <title>)
      	MonImage.Nom		= _RécupèreBaliseTitre(sUnChemin)
      	// - le nom du fichier (ou une partie du nom)
      	//MonImage.Nom		= ExtraitChaîne(fExtraitChemin(sUnChemin, fFichier), 2, "-")
      	
      	// Il est possible de forcer une taille par défaut pour l'objet lorsqu'il sera ajouté au diagramme
      	MonImage.Largeur	= 120
      	MonImage.Hauteur	= 120
      	
      	// Ajoute cette image à la bibliothèque
      	MaBibliothèque.Forme.Ajoute(MonImage)
      	
      FIN
      
      sFichier est une chaîne = fRepDonnéesUtilisateur(frdDéfaut) + fSep() + sNom + ".wddiag"
      
      DiagrammeSauve(MaBibliothèque, sFichier)
      
      
      	PROCÉDURE INTERNE _RécupèreBaliseTitre(sCheminSVG est une chaîne)
      		
      		// Charge le contenu du fichier SVG dans un xmlDocument
      		MonSVG est un xmlDocument = XMLOuvre(sCheminSVG)
      		
      		// Récupère le contenu de la balise titre
      		RENVOYER MonSVG.svg.title
      		
      	FIN
      
      renvoyer sFichier
     type : 458752
